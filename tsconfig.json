{
	"compilerOptions": {
		"module": "CommonJS",
		"noImplicitAny": false,
		"removeComments": false,
		"jsx": "preserve",
		"lib": ["DOM"],
		"preserveConstEnums": true,
		"outDir": "dist",
		"sourceMap": true,
		"target": "ES2015"
	},
	"include": ["src/**/*.ts", "src/**/*.tsx"],
	"exclude": ["node_modules"]
}
